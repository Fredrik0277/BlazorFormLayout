@inherits BsControlBase<string>
<BsGroup Label="@Label" Id="@Id">
    <InputSelect @bind-Value="@Value" class="form-control" id="@Id" disabled="@(Mode == EditMode.ReadOnly)" >
        @foreach (var item in ValueOptions)
        {
            <option>@item</option>
        }
    </InputSelect>
</BsGroup>
@code
{
    [Parameter] public IEnumerable<string> ValueOptions { get; set; }

}